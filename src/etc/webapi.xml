<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/webapidocuments/upload" method="POST">
        <service class="Zamoroka\WebapiDocuments\Api\PdfInterface" method="upload" />
        <resources>
            <resource ref="Zamoroka_WebapiDocuments::upload" />
        </resources>
    </route>
</routes>
